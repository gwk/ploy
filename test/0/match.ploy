main = {

  tInt = fn Int%Int
    match $
      0 ? 0
      (1) ? 1
      x ? -1
      / -2;;

  assert.eq(0 tInt.0)
  assert.eq(1 tInt.1)
  assert.eq(-1 tInt.-1)

  tStr = fn Str%Str
    match $
      "a" ? "a"
      ("b") ? "b"
      x ? "x"
      / "z";;

  assert.eq("a" tStr."a")
  assert.eq("b" tStr."b")
  assert.eq("x" tStr."c")


}
