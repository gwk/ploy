main = {
  assert.eq(0 add(0 0))
  assert.eq(3 add(1 2))

  assert.eq(0 sub(0 0))
  assert.eq(1 sub(3 2))
  assert.eq(-1 sub(2 3))

  assert.eq(0 mul(0 0))
  assert.eq(2 mul(1 2))

  assert.eq(0 div(0 1))
  assert.eq(2 div(4 2))

  assert.eq( 1 mod( 3  2))
  assert.eq( 1 mod(-3  2))
  assert.eq(-1 mod( 3 -2))
  assert.eq(-1 mod(-3 -2))

  assert.eq( 1 rem( 3  2))
  assert.eq(-1 rem(-3  2))
  assert.eq( 1 rem( 3 -2))
  assert.eq(-1 rem(-3 -2))

  assert.eq(1 inc.0)
  assert.eq(0 dec.1)
}
