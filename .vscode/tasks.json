{
  "version": "2.0.0",
  "command": ".vscode/run-args.sh",
  "echoCommand": false,
  "type": "process",
  "presentation": {},
  "suppressTaskName": true,
  "tasks": [
    { "taskName": "_default",
      "args": ["make"],
      "group": "build",
      "problemMatcher": []
    },
    { "taskName": "build",
      "args": ["make", "build"]
    },
    { "taskName": "test-current",
      "args": ["sh/test.sh", "${relativeFile}"],
      "group": "test"
    },
    { "taskName": "test",
      "args": ["make", "test"]
    }
  ]
}
